{
  "type": "mongodb",
  "host": "localhost",
  "database": "BLE-beacon-database",
  "synchronize": true,
  "logging": false,
  "entities": [
    "src/typeORM/entity/*.ts"
  ],
  "subscribers": [
    "src/typeORM/subscriber/*.ts"
  ],
  "migrations": [
    "src/typeORM/migration/*.ts"
  ],
  "cli": {
    "entitiesDir": "src/entity",
    "migrationsDir": "src/migration",
    "subscribersDir": "src/subscriber"
  }
}